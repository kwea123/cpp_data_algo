#include <iostream>
#include <time.h> //set time seed
#include <stdlib.h>
using namespace std;

void Knuth(int* a, int n){
	for(int i=1;i<n;i++){
		int j = rand()%(i+1); // j in 0..i
		int tmp = a[i];
		&a[i] = &a[j];
		&a[j] = &a[i];
	}
}

int main() {
	srand(time(NULL));
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
