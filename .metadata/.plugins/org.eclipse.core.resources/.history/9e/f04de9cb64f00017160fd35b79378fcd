#include <iostream>
#include <stdexcept>
using namespace std;

struct PriorityQueue{
	int* elements;
	int size;

	PriorityQueue(int capacity){
		elements = new int[capacity];
		size = 0;
	}

	bool isEmpty(){
		return size == 0;
	}

	int getMin(){
		if(size==0)
			throw exception("No such element");
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
