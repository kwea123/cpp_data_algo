#ifndef GRAPHHW_H_
#define GRAPHHW_H_

#include <vector>

struct Ocean;
struct Cell;

int N = 41;

struct Ocean{
	std::vector<std::vector<Cell*> > cells;
	Ocean(std::vector<std::vector<Cell*> > cells){
		this->cells = cells;
	}
};

struct Cell{
	int x, y;

	Cell(int x, int y);

	std::vector<Cell*> neighbors(Ocean*);

	bool operator==(const Cell& b) const{
		return x==b.x && y==b.y;
	}

};


#endif /* GRAPHHW_H_ */
