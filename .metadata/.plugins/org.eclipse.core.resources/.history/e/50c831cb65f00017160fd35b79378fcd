#include <iostream>
#include <stdexcept>
using namespace std;

struct PriorityQueue{
	int* elements;
	int size;

	PriorityQueue(int capacity){
		elements = new int[capacity];
		size = 0;
	}

	bool isEmpty(){
		return size == 0;
	}

	int getMin(){
		if(size==0)
			throw exception("No such element");
		return elements[0];
	}

	void moveUp(int x, int i) {
		while (i > 0) {
			int parent_i = (i - 1) / 2;
			int y = elements[parent_i];
			if (y <= x)
				break;
			elements[i] = y; //insert y to this place
			i = parent_i; //and move the node up, keep comparing
		}
		elements[i] = x;
	}

	void add(int x) {
		moveUp(x, size++);
	}

	void moveDown(int x, int i) {
		int n = size;
		while (true) {
			int j = 2 * i + 1;
			if (j >= n)
				break;
			if (j + 1 < n && elements[j] > elements[j + 1]) j++;
			if (x <= this.elts[j]) break;
			this.elts[i] = this.elts[j];
			i = j;
		}
		this.elts[i] = x;
	}



};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
