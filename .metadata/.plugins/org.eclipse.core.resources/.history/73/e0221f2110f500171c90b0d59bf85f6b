#include <stdlib.h>
#include <iostream>
using namespace std;

int N = 15;

bool check(int* col, int r){
	for(int q=0;q<r;q++)
		if(col[q]==col[r]||abs(col[q]-col[r])==r-q)
			return false;
	return true;
}

int countS(int* col, int r){
	if(r==N)
		return 1;
	int res = 0;
	for(int c=0;c<N;c++) {
		col[r] = c;
		if (check(col, r))
			res += countS(col, r + 1);
	}
	return res;
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
