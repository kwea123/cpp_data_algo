#include <iostream>
using namespace std;

string s1 = "afgerbc";
string s2 = "rdgnrqec";
int length[10][10];
void LCS(){
	for(int i=0;i<=s1.length();i++)
		length[i][0] = 0;
	for(int j=0;j<=s2.length();j++)
		length[0][j] = 0;
	for(int i=1;i<=s1.length();i++)
		for(int j=1;j<=s2.length();j++){
			if(s1[i-1]==s2[j-1])
				length[i][j] = 1+length[i-1][j-1];
			else
				length[i][j] = max(length[i-1][j], length[i][j-1]);
		}
}

int main() {
	cout<<length[3][3]<<endl;
	return 0;
}
