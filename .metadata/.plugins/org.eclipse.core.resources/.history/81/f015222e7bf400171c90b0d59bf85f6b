#include <iostream>
#include <time.h>
using namespace std;

struct BIT{
	int N;
	int* tree;

	BIT(int N){
		this->N = N;
		this->tree = new int[N+1];
	}

	void add(int idx ,int val){ //adds val to a[idx]
	    while (idx <= N){
	        tree[idx] += val;
	        idx += (idx & -idx);
	    }
	}

	int cumulativeSum(int idx){ //returns a[1]+...+a[idx]
		int sum = 0;
		while (idx > 0){
			sum += tree[idx];
			idx -= (idx & -idx);
		}
		return sum;
	}

	int rangeSum(int l, int r){ //returns a[l]+...+a[r]
		return cumulativeSum(r) - cumulativeSum(l-1);
	}
};

int main() {
	int N = 1000;
	BIT bit = BIT(N);

	clock_t begin = clock();

	for(int i=1;i<=N;i++)
		bit.add(i, i);
	for(int i=1;i<N;i++)
		cout<<bit.cumulativeSum(i)<<endl;

	cout<<clock() - begin<<endl;

	return 0;
}
